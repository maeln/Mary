#include <string>
#include <vector>
#include <sstream>

#include "notes.hxx"

class MusicMetadata
{
	public:
		std::string title;
		std::string artists;
		std::string genre;
		int length;
		int bpm;
		std::vector<Notes> topNotes;

		std::string toString()
		{
			std::stringstream ss;
			ss << "Title: " << title << std::endl;
		}
};
